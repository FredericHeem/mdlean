{
  "name": "mdlean",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
  },
  "peerDependencies": {
    "glamor": "*",
    "glamorous": "*",
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.0",
    "react-scripts": "1.0.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build:lib": "mkdirp build && webpack --progress --colors",
    "build:min": "mkdirp build && cross-env LMD_ENV=production webpack -p --progress --colors"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "test": {
        "presets": [
          "es2015"
        ]
      }
    }
  }
}